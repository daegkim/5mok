extends layout

block content
  script(src='/javascripts/socket_client.js')
  h1= title
  p Welcome to #{title}
  button#btnCreateRoom Create Room
  div#divRoomList

  div#div_board
    table
      tbody
        -for(var i=0; i<18; i++)
            tr
              -for(var j=0; j<18; j++)
                  td(id = "td" + (i*18 + j))
